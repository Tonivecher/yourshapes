{
  "name": "yourshapes",
  "private": true,
  "version": "0.0.0",
  "description": "YourShapes monorepo",
  "workspaces": [
    "webapp-pure-form-7dzsf1",
    "webapp-pure-form-7dzsf1/apps/*"
  ],
  "scripts": {
    "dev": "cd webapp-pure-form-7dzsf1 && bun run dev",
    "dev:website": "cd webapp-pure-form-7dzsf1 && bun run dev:website",
    "build:website": "cd webapp-pure-form-7dzsf1 && bun run build:website",
    "build:website:dev": "cd webapp-pure-form-7dzsf1 && bun run build:website:dev",
    "preview:website": "cd webapp-pure-form-7dzsf1 && bun run preview:website",
    "timeweb:mcp": "bash scripts/run-timeweb-mcp-server.sh",
    "timeweb:mcp:call": "node scripts/timeweb-mcp-cli.mjs",
    "build": "bash scripts/timeweb-build.sh",
    "preview": "cd webapp-pure-form-7dzsf1/apps/website && npm run preview -- --host --port 8080"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "timeweb-mcp-server": "^0.1.3"
  }
}
